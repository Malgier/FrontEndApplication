services:
  auth:
    container_name: auth
    image: auth-service
  customer-ordering-service:
    container_name: customer-ordering
    image: customer-ordering-service
    links:
    - products
  frontendapp:
    build:
      args:
        source: obj/Docker/empty/
      context: C:\Users\Jason\Source\Repos\FrontEndApplication\FrontEndApp\FrontEndApp
      dockerfile: Dockerfile
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: thamco:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.0/FrontEndApp.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    links:
    - auth
    - customer-ordering-service
    - products
    - staff-ordering-service
    ports:
    - 80/tcp
    volumes:
    - C:\Users\Jason\Source\Repos\FrontEndApplication\FrontEndApp\FrontEndApp:/app:rw
    - C:\Users\Jason\vsdbg:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Jason\.nuget\packages:/root/.nuget/packages:ro
  products:
    container_name: products
    image: product-service
  reverse-proxy:
    container_name: reverse-proxy
    depends_on:
    - customer-ordering-service
    - staff-ordering-service
    image: reverse-proxy
    ports:
    - 80:80/tcp
  staff-ordering-service:
    container_name: staff-ordering
    image: staff-ordering-service
    links:
    - products
    - third-party-repo
  third-party-repo:
    container_name: third-party-repo
    image: third-party-repo
version: '3.0'