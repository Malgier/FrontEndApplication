services:
  auth:
    container_name: auth
    image: auth-service
  customer-ordering-service:
    container_name: customer-ordering
    image: customer-ordering-service
    links:
    - products
  frontendapp:
    build:
      context: C:\Users\Jason\Source\Repos\FrontEndApplication\FrontEndApp\FrontEndApp
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: thamco
    links:
    - auth
    - customer-ordering-service
    - products
    - staff-ordering-service
    ports:
    - 80/tcp
  products:
    container_name: products
    image: product-service
  reverse-proxy:
    container_name: reverse-proxy
    depends_on:
    - customer-ordering-service
    - staff-ordering-service
    image: reverse-proxy
    ports:
    - 80:80/tcp
  staff-ordering-service:
    container_name: staff-ordering
    image: staff-ordering-service
    links:
    - products
    - third-party-repo
  third-party-repo:
    container_name: third-party-repo
    image: third-party-repo
version: '3.0'